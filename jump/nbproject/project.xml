<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>jump</name>
            <properties>
                <property name="ant.script">build/build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>classes_api</label>
                    <type>java</type>
                    <location>src/share/api/classes</location>
                </source-folder>
                <source-folder>
                    <label>classes_impl_contentstore</label>
                    <type>java</type>
                    <location>src/share/impl/contentstore/classes</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>dist</target>
                </action>
                <action name="javadoc">
                    <script>${ant.script}</script>
                    <target>doc</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="rebuild">
                    <script>${ant.script}</script>
                    <target>clean</target>
                    <target>dist</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>classes_api</label>
                        <location>src/share/api/classes</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>classes_impl_contentstore</label>
                        <location>src/share/impl/contentstore/classes</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="clean"/>
                    <ide-action name="rebuild"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src/share/api/classes</package-root>
                <package-root>src/share/impl/contentstore/classes</package-root>
                <source-level>1.4</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
