<project basedir="." default="all" name="jump-impl-store">

  <property name="s.share.impl.dir" value="${s.share.dir}/impl/contentstore"/>

  <target name="all" depends="compile-contentstore-module"/>
  
  <target name="compile-contentstore-module">
      <javac destdir="${d.classes.dir}"
             srcdir="${s.share.impl.dir}/classes"
           source="1.4"
           deprecation="true"
           target="1.4"
           debug="true">
           <include name="com/sun/jumpimpl/module/contentstore/*.java" />
    </javac>
  </target>

</project>
